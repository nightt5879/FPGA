############################
# On-board Slide Switches  #
############################
set_property PACKAGE_PIN N15 [get_ports {swt[0]}]
set_property IOSTANDARD LVCMOS33 [get_ports {swt[0]}]

set_property PACKAGE_PIN N16 [get_ports {swt[1]}]
set_property IOSTANDARD LVCMOS33 [get_ports {swt[1]}]

set_property PACKAGE_PIN T17 [get_ports {swt[2]}]
set_property IOSTANDARD LVCMOS33 [get_ports {swt[2]}]

set_property PACKAGE_PIN R17 [get_ports {swt[3]}]
set_property IOSTANDARD LVCMOS33 [get_ports {swt[3]}]

############################
# On-board LED             #
############################
set_property PACKAGE_PIN M14 [get_ports {led[0]}]
set_property IOSTANDARD LVCMOS33 [get_ports {led[0]}]

set_property PACKAGE_PIN M15 [get_ports {led[1]}]
set_property IOSTANDARD LVCMOS33 [get_ports {led[1]}]

set_property PACKAGE_PIN K16 [get_ports {led[2]}]
set_property IOSTANDARD LVCMOS33 [get_ports {led[2]}]

set_property PACKAGE_PIN J16 [get_ports {led[3]}]
set_property IOSTANDARD LVCMOS33 [get_ports {led[3]}]
